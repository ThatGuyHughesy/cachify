{
  "name": "Cachify",
  "version": "0.1.0-SNAPSHOT",
  "description": "Turn your Spotify playlist into a cache.",
  "keywords": [
    "spotify",
    "playlist",
    "cache",
    "react",
    "redux",
    "webpack",
    "sass",
    "es6",
    "node",
    "mongo"
  ],
  "author": "Conor Hughes <hello@conorhughes.me>",
  "license": "MIT",
  "main": "server.js",
  "scripts": {
    "start": "node ./src/server/index.js",
    "server": "nodemon ./src/server/index.js",
    "lint": "eslint --fix . --ext .js ./src",
    "format": "prettier-eslint --list-different 'src/**/*.js'",
    "test": "NODE_ENV=test mocha --timeout 3000 ./src/server/models/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThatGuyHughesy/cachify.git"
  },
  "bugs": {
    "url": "https://github.com/ThatGuyHughesy/cachify/issues"
  },
  "homepage": "https://github.com/ThatGuyHughesy/cachify#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.4",
    "mongoose": "^5.4.6",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-spotify": "^1.0.1",
    "spotify-web-api-node": "^4.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "sinon": "^7.2.3",
    "supertest": "^3.4.2"
  }
}
