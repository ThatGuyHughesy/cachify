{
  "name": "cachify",
  "version": "0.1.0-SNAPSHOT",
  "description": "Turn your Spotify playlist into a cache.",
  "keywords": [
    "spotify",
    "playlist",
    "cache",
    "react",
    "redux",
    "webpack",
    "sass",
    "es6",
    "node",
    "mongo"
  ],
  "author": "Conor Hughes <hello@conorhughes.me>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
